<div class="form-group">
  <%= f.label :key_note %><br />
  <%= f.text_field :key_note, autofocus: true %>
</div>

<div class="form-group">
  <%= f.label :date %><br />
  <%= f.date_field :date, min: Date.today, autofocus: true %>
</div>

<div class="form-group">
  <%= f.label :start_time %>
  <%= f.time_field :start_time, autocomplete: "off" %>
</div>

<div class="form-group">
  <%= f.label :duration %><br />
  <%= f.number_field :duration, autocomplete: "off" %>
</div>

<div class="form-group">
  <%= f.label :status %><br />
  <%= f.select :status, options_for_select(@status_list.nil? ? ['not available']: @status_list)%>
</div>

<div class="form-group">
  <%= f.label :venue %><br />
  <%= f.select :venue_id, options_for_select(@venues.nil? ? ['not available']: @venues)%>
</div>

<div class="actions">
  <%= f.submit "Submit" %>
</div>